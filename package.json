{
  "name": "@lapalma24/mcp-server",
  "version": "1.0.0",
  "description": "Official MCP server for La Palma 24 vacation rentals - Search and discover properties in La Palma, Canary Islands",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "lapalma24-mcp": "./dist/index.js"
  },
  "mcpName": "net.la-palma24/vacation-rentals",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:http": "node dist/index-http.js",
    "dev": "tsc && node dist/index.js",
    "dev:http": "tsc && node dist/index-http.js",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "vacation-rentals",
    "canary-islands",
    "la-palma",
    "property-search",
    "booking",
    "travel",
    "spain",
    "accommodation"
  ],
  "author": {
    "name": "TuneraSoft - La Palma 24",
    "email": "info@la-palma24.net",
    "url": "https://www.la-palma24.net"
  },
  "license": "MIT",
  "homepage": "https://github.com/La-Palma-24/lapalma24-mcp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/La-Palma-24/lapalma24-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/La-Palma-24/lapalma24-mcp/issues"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "server.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3"
  }
}
